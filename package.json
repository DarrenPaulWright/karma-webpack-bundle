{
	"name": "karma-webpack-bundle",
	"version": "0.4.0",
	"author": {
		"name": "Darren Paul Wright",
		"email": "Darren@DarrenPaulWright.com",
		"url": "https://www.darrenpaulwright.com"
	},
	"description": "A personalized bundle of karma libraries and configs",
	"main": "index.js",
	"files": [
		"/src"
	],
	"scripts": {
		"test": "eslint **/*.js && karma start --single-run && npm run bench",
		"test-watch": "karma start",
		"bench": "karma start karma.bench.conf.js",
		"preversion": "npm test && npm run docs && git add --all && git diff --staged --quiet || git commit -m \"Updating docs\"",
		"postversion": "npm publish",
		"postpublish": "git push --follow-tags",
		"pre-clean": "rm -rf node_modules && del /f package-lock.json",
		"clean": "npm run pre-clean && npm install",
		"update": "ncu -u && npm install",
		"update-clean": "npm run pre-clean && npm run update",
		"update-commit": "npm run update && npm test && git add --all && git diff --staged --quiet || git commit -m \"Updating dependencies\"",
		"update-patch": "npm run update-commit && npm version patch",
		"docs2": "jsdoc2md ./index.js src/*.js > README.md -t node_modules/dmd-readable/overview.hbs --plugin dmd-readable/overview-plugin.js",
		"docs": "jsdoc2md ./index.js src/*.js > README.md -t  -t node_modules/dmd-readable/docs.hbs --plugin dmd-readable"
	},
	"badges": {
		"npm": {
			"image": "https://img.shields.io/npm/v/{{name}}.svg",
			"url": "https://npmjs.com/package/{{name}}"
		},
		"deps": {
			"image": "https://david-dm.org/darrenpaulwright/{{name}}.svg",
			"url": "https://david-dm.org/darrenpaulwright/{{name}}"
		},
		"size": {
			"base": "https://packagephobia.now.sh/",
			"image": "badge?p={{name}}",
			"url": "result?p={{name}}"
		},
		"vulnerabilities": {
			"base": "https://snyk.io/test/github/DarrenPaulWright/{{name}}",
			"image": "/badge.svg?targetFile=package.json",
			"url": "?targetFile=package.json"
		},
		"license": {
			"image": "https://img.shields.io/github/license/DarrenPaulWright/{{name}}.svg",
			"url": "https://npmjs.com/package/{{name}}/LICENSE.md"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/DarrenPaulWright/karma-webpack-bundle.git"
	},
	"keywords": [
		"karma",
		"webpack",
		"benchmark"
	],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/DarrenPaulWright/karma-webpack-bundle/issues"
	},
	"homepage": "https://github.com/DarrenPaulWright/karma-webpack-bundle#readme",
	"dependencies": {
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.9.0",
		"babel-eslint": "^10.1.0",
		"babel-loader": "^8.1.0",
		"babel-plugin-istanbul": "^6.0.0",
		"benchmark": "^2.1.4",
		"chalk": "^3.0.0",
		"display-value": "^1.7.2",
		"eslint": "^6.8.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-import": "^2.20.2",
		"eslint-plugin-jsdoc": "^22.1.0",
		"eslint-plugin-mocha": "^6.3.0",
		"eslint-plugin-unicorn": "^18.0.1",
		"is-travis": "^2.0.0",
		"karma": "^4.4.1",
		"karma-benchmark": "^1.0.4",
		"karma-benchmarkjs-reporter": "^1.0.0",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage": "^2.0.1",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.3.0",
		"karma-mocha": "^1.3.0",
		"karma-mocha-reporter": "^2.2.5",
		"karma-webpack": "^4.0.2",
		"mocha": "^7.1.1",
		"null-loader": "^3.0.0",
		"strip-ansi": "^6.0.0",
		"test-runner-config": "^1.0.0",
		"type-enforcer": "^1.1.1",
		"wallaby-webpack": "^3.9.15",
		"webpack": "^4.42.1"
	},
	"devDependencies": {
		"dmd-readable": "^1.2.1",
		"jsdoc-to-markdown": "^5.0.3"
	}
}
